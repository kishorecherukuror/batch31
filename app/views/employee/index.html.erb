<h1>Employee List</h1>

<table border="1" width="50%" cellpadding="5" cellspacing="5">
	<tr>
		<th>Name</th>
		<th>Age</th>
		<th>DOB</th>
		<th>Gender</th>
	</tr>

<% @emp.each do |i| %>
<tr>
	<td><%= i.name %></td>
	<td><%= i.age %></td>
	<td><%= i.dob %></td>
	<td><%= i.gender %></td>
</tr>
<% end %>
</table>